          cn.yiyingli.Servlet.UpLoadImageServlet
       </servlet-class>
  </servlet>
		
   <servlet-mapping>
    <servlet-name>UploadImage</servlet-name>
    <url-pattern>/upimage</url-pattern>
  </servlet-mapping>
  
  <servlet>
