<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd" id="WebApp_ID" version="3.0">
  <display-name>OneMile</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  
   <servlet>
    <description>管理</description>
    <display-name>Manage</display-name>
    <servlet-name>Manage</servlet-name>
    <servlet-class>
          cn.yiyingli.Servlet.ManageServlet
       </servlet-class>
  </servlet>
		
   <servlet-mapping>
    <servlet-name>Manage</servlet-name>
    <url-pattern>/manage</url-pattern>
  </servlet-mapping>
  
  
  <servlet>
    <description>更改头像</description>
    <display-name>ChangeIcon</display-name>
    <servlet-name>ChangeIcon</servlet-name>
    <servlet-class>
          cn.yiyingli.Servlet.ChangeIconServlet
       </servlet-class>
  </servlet>
		
   <servlet-mapping>
    <servlet-name>ChangeIcon</servlet-name>
    <url-pattern>/changeIcon</url-pattern>
  </servlet-mapping>
  
  <servlet> 
        <servlet-name>validateCodeServlet</servlet-name> 
        <servlet-class>cn.yiyingli.Servlet.ValidateCodeServlet</servlet-class> 
    </servlet>     
    <servlet-mapping> 
        <servlet-name>validateCodeServlet</servlet-name> 
        <url-pattern>*.images</url-pattern> 
    </servlet-mapping> 
    
   <servlet>
    <description>上传头像图片</description>
    <display-name>UploadImage</display-name>
