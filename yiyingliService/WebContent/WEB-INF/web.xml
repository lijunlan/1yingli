  </servlet-mapping>
  
  <servlet>
    <description>上传背景</description>
    <display-name>UploadBackground</display-name>
    <servlet-name>UploadBackground</servlet-name>
    <servlet-class>
          cn.yiyingli.Servlet.UpLoadBackgroundServlet
       </servlet-class>
  </servlet>
		
   <servlet-mapping>
    <servlet-name>UploadBackground</servlet-name>
    <url-pattern>/upbg</url-pattern>
  </servlet-mapping>
  
